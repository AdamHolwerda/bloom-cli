{
    
      "env": {
        "es6": true,
        "browser": true,
        "commonjs": true,
        "jquery": true,
        "node": true
      },
    
      "plugins": [
        "react"
      ],
    
      "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
      ],
    
      "globals": {
        "console": true,
        "process": true,
        "React": true,
        "ReactDOM": true,
        "_": true
      },
      "parser" : "babel-eslint",
      "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
          "jsx": true,
          "experimentalObjectRestSpread": true
        }
      },
    
      "rules": {
        // style
        // http://eslint.org/docs/rules/array-bracket-spacing.html
        "array-bracket-spacing": [
          "warn",
          "never"
        ],
    
        // style
        // http://eslint.org/docs/rules/arrow-parens.html
        "arrow-parens": [
          "warn"
        ],
    
        // style
        // http://eslint.org/docs/rules/arrow-spacing.html
        "arrow-spacing": [
          "warn",
          {
            "before": true,
            "after": true
          }
        ],
    
        // style
        // http://eslint.org/docs/rules/brace-style.html
        "brace-style": [
          "warn",
          "1tbs"
        ],
    
        // style
        // http://eslint.org/docs/rules/camelcase.html
        "camelcase": [
          "warn",
          {
            "properties": "always"
          }
        ],
    
        // style
        // http://eslint.org/docs/rules/comma-dangle
        "comma-dangle": [
          "warn",
          "never"
        ],
    
        // style
        // http://eslint.org/docs/rules/dot-notation.html
        "dot-notation": [
          "warn"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/eqeqeq.html
        "eqeqeq": [
          "error",
          "always"
        ],
    
        // style
        // http://eslint.org/docs/rules/indent.html
        "indent": [
          "warn",
          4,
          {
            "SwitchCase": 1
          }
        ],
    
        // style
        // http://eslint.org/docs/rules/keyword-spacing.html
        "keyword-spacing": [
          "warn",
          {
            "before": true,
            "after": true
          }
        ],
    
        // style
        // http://eslint.org/docs/rules/newline-after-var
        "newline-after-var": [
          "warn",
          "always"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-case-declarations.html
        "no-case-declarations": [
          "error"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-confusing-arrow
        "no-confusing-arrow": [
          "error",
          {
            "allowParens": true
          }
        ],
    
        // style
        // http://eslint.org/docs/rules/no-console
        "no-console": [
          "warn",
          {
            "allow": [ "warn" ] 
          }
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-const-assign.html
        "no-const-assign": [
          "error"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-dupe-class-members
        "no-dupe-class-members": [
          "error"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-duplicate-imports
        "no-duplicate-imports": [
          "error"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-loop-func.html
        "no-loop-func": [
          "error"
        ],
    
        // style
        // http://eslint.org/docs/rules/no-nested-ternary.html
        "no-nested-ternary": [
          "warn"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-param-reassign.html
        "no-param-reassign": [
          "error"
        ],
    
        // style
        // http://eslint.org/docs/rules/no-unneeded-ternary.html
        "no-unneeded-ternary": [
          "warn"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-useless-constructor
        "no-useless-constructor": [
          "error"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/no-var.html
        "no-var": [
          "error"
        ],
    
        // style
        // http://eslint.org/docs/rules/object-curly-spacing.html
        "object-curly-spacing": [
          "warn",
          "always"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/one-var.html
        "one-var": [
          "error",
          "never"
        ],
    
        // style
        // http://eslint.org/docs/rules/prefer-arrow-callback.html
        "prefer-arrow-callback": [
          "warn"
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/prefer-const.html
        "prefer-const": [
          "error"
        ],
    
        // style
        // http://eslint.org/docs/rules/quotes.html
        "quotes": [
          "warn",
          "single"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-boolean-value.md
        "react/jsx-boolean-value": [
          "warn",
          "always"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-closing-bracket-location.md
        "react/jsx-closing-bracket-location": [
          "warn",
          "tag-aligned"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-curly-spacing.md
        "react/jsx-curly-spacing": [
          "warn",
          "never"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-equals-spacing.md
        "react/jsx-equals-spacing": [
          "warn",
          "never"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-indent.md
        "react/jsx-indent": [
          "warn",
          4
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-indent-props.md
        "react/jsx-indent-props": [
          "warn",
          4
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-key.md
        "react/jsx-key": [
          "error"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-max-props-per-line.md
        "react/jsx-max-props-per-line": [
          "warn",
          {
            "maximum": 3
          }
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-no-duplicate-props.md
        "react/jsx-no-duplicate-props": [
          "error"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-no-target-blank.md
        "react/jsx-no-target-blank": [
          "error"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-no-undef.md
        "react/jsx-no-undef": [
          "error"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-space-before-closing.md
        "react/jsx-space-before-closing": [
          "warn",
          "always"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/jsx-wrap-multilines.md
        "react/jsx-wrap-multilines": [
          "warn"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/no-deprecated.md
        "react/no-deprecated": [
          "error"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/no-did-mount-set-state.md
        "react/no-did-mount-set-state": [
          "warn"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/no-did-update-set-state.md
        "react/no-did-update-set-state": [
          "warn"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/no-direct-mutation-state.md
        "react/no-direct-mutation-state": [
          "warn"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/no-multi-comp.md
        "react/no-multi-comp": [
          "warn",
          {
            "ignoreStateless": false
          }
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/prefer-stateless-function.md
        "react/prefer-stateless-function": [
          "error"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/prefer-es6-class.md
        "react/prefer-es6-class": [
          "warn",
          "always"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/prop-types.md
        "react/prop-types": [
          "error"
        ],
    
        // error prevention
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/require-render-return.md
        "react/require-render-return": [
          "error"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/self-closing-comp.md
        "react/self-closing-comp": [
          "warn"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/sort-comp.md
        "react/sort-comp": [
          "warn"
        ],
    
        // style
        // https://github.com/yannickcr/eslint-plugin-react/blob/master/docs/rules/sort-prop-types.md
        "react/sort-prop-types": [
          "warn",
          {
            "ignoreCase": true
          }
        ],
    
        // error prevention
        // http://eslint.org/docs/rules/semi.html
        "semi": [
          "error",
          "always"
        ],
    
        // style
        // http://eslint.org/docs/rules/space-before-blocks.html
        "space-before-blocks": [
          "warn",
          "always"
        ],
    
        // style
        // http://eslint.org/docs/rules/space-infix-ops.html
        "space-infix-ops": [
          "warn"
        ],
    
        // style
        // http://eslint.org/docs/rules/space-in-parens.html
        "space-in-parens": [
          "warn"
        ],
    
        // style
        // http://eslint.org/docs/rules/wrap-iife.html
        "wrap-iife": [
          "warn"
        ]
      }
    
    
    }